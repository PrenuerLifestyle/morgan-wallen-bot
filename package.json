{
  "name": "morgan-wallen-celebrity-bot",
  "version": "1.0.0",
  "description": "Official Morgan Wallen Celebrity Assistant Bot - Full-featured Telegram bot for bookings, tickets, memberships, and fan engagement",
  "main": "bot.js",
  "scripts": {
    "start": "node bot.js",
    "dev": "nodemon bot.js",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "db:init": "node scripts/init-db.js",
    "db:seed": "node scripts/seed-db.js",
    "db:backup": "pg_dump $DATABASE_URL > backups/backup-$(date +%Y%m%d-%H%M%S).sql",
    "deploy:railway": "railway up",
    "deploy:heroku": "git push heroku main"
  },
  "keywords": [
    "telegram-bot",
    "celebrity-assistant",
    "booking-system",
    "stripe-payments",
    "event-management"
  ],
  "author": "Morgan Wallen Management Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "telegraf": "^4.15.0",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "stripe": "^14.10.0",
    "dotenv": "^16.3.1",
    "nodemailer": "^6.9.7",
    "googleapis": "^129.0.0",
    "axios": "^1.6.2",
    "node-cron": "^3.0.3",
    "bull": "^4.12.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "qrcode": "^1.5.3",
    "moment-timezone": "^0.5.43",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "@types/node": "^20.10.6"
  },
  "nodemonConfig": {
    "watch": [
      "bot.js",
      ".env"
    ],
    "ext": "js,json",
    "ignore": [
      "node_modules/**",
      "backups/**"
    ],
    "delay": "2000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourorg/morgan-wallen-bot"
  }
}
